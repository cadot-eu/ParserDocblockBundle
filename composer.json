{
    "name": "cadot.eu/parser-docblock-bundle",
    "description": "Parser docblock for Doctrine",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.0",
        "symfony/http-kernel": "^5.3|^6.0",
        "symfony/framework-bundle": "^5.3|^6.0",
        "symfony/dependency-injection": "^5.3|^6.0",
        "doctrine/dbal": "4.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0",
        "symfony/browser-kit": "^5.0",
        "symfony/phpunit-bridge": "6.1.x-dev",
        "symplify/easy-coding-standard": "^10.2"
    },
    "homepage": "https://github.com/cadot-eu/parsercommentbundle#readme",
    "keywords": [
        "symfony",
        "extension",
        "bundle",
        "service",
        "doctrine",
        "parser",
        "docblock",
        "cadot.eu"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cadoteu\\ParserDocblockBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "cadot.eu",
            "email": "composer@cadot.eu"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "test": "./vendor/bin/simple-phpunit tests --bootstrap tests/bootstrap.php --verbose"
    }
}
